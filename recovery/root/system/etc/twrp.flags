# mount point          fstype   device                                                   flags
/cache                 ext4     /dev/block/platform/bootdevice/by-name/cache             flags=display="Cache";
/mnt/vendor/protect_f  ext4     /dev/block/platform/bootdevice/by-name/protect1          flags=display="Protect_f";backup=1;
/mnt/vendor/protect_s  ext4     /dev/block/platform/bootdevice/by-name/protect2          flags=display="Protect_s";backup=1;
/mnt/vendor/nvdata     ext4     /dev/block/platform/bootdevice/by-name/nvdata            flags=display="Nvdata";
/mnt/vendor/nvcfg      ext4     /dev/block/platform/bootdevice/by-name/nvcfg             flags=display="Nvcfg";
/persistent            emmc     /dev/block/platform/bootdevice/by-name/frp               defaults defaults
/nvram                 emmc     /dev/block/platform/bootdevice/by-name/nvram             flags="Nvram";backup=1;flashimg=1;
/proinfo               emmc     /dev/block/platform/bootdevice/by-name/proinfo           defaults defaults
/boot                  emmc     /dev/block/platform/bootdevice/by-name/boot              flags=display="Boot";flashimg=1;
/recovery              emmc     /dev/block/platform/bootdevice/by-name/recovery          flags=display="Recovery";flashimg=1;
/gz1                   emmc     /dev/block/platform/bootdevice/by-name/gz1               defaults defaults
/gz2                   emmc     /dev/block/platform/bootdevice/by-name/gz2               defaults defaults
/spmfw                 emmc     /dev/block/platform/bootdevice/by-name/spmfw             defaults defaults
/odmdtbo               emmc     /dev/block/platform/bootdevice/by-name/odmdtbo           defaults defaults
/dtbo                  emmc     /dev/block/platform/bootdevice/by-name/dtbo              defaults defaults
/vbmeta                emmc     /dev/block/platform/bootdevice/by-name/vbmeta            defaults defaults
/tee2                  emmc     /dev/block/platform/bootdevice/by-name/tee2    
/sspm_1                emmc     /dev/block/platform/bootdevice/by-name/sspm_1  
/sspm_2                emmc     /dev/block/platform/bootdevice/by-name/sspm_2 

# Removable
/external_sd	 	  auto 	/dev/block/mmcblk1p1	/dev/block/mmcblk1
/usb-otg               vfat     /dev/block/sda1         /dev/block/sdd                     flags=storage;wipeingui;removable